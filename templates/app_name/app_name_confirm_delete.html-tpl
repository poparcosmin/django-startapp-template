{% extends 'base.html' %}

{% block content %}
    <h1>Confirm Delete</h1>
    <p>Are you sure you want to delete "{{ object }}"?</p>

    <form method="post">
        {% csrf_token %}
        <input type="submit" value="Yes, delete." class="btn btn-danger">
        <a href="{% url 'app_name_detail' object.pk %}" class="btn btn-secondary">Cancel</a>
    </form>
{% endblock %}
